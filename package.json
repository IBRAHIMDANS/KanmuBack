{
  "name": "kamnu",
  "version": "0.0.1",
  "description": "kanmu api",
  "author": "Ibrahima DANSOKO <ibrahima.dansoko@outlook.com",
  "contributors": [
    {
      "name": "Jeremie Dupas",
      "url": "https://gitlab.com/dupasj"
    }
  ],
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:prod",
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "migration:create": "ts-node node_modules/.bin/typeorm migration:create",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate",
    "cache:clear": "ts-node ./node_modules/.bin/typeorm cache:clear"
  },
  "dependencies": {
    "@admin-bro/nestjs": "^1.0.0",
    "@admin-bro/typeorm": "^1.4.0",
    "@godaddy/terminus": "^4.4.1",
    "@golevelup/nestjs-webhooks": "^0.2.6",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.4",
    "admin-bro": "^3.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto-js": "^4.0.0",
    "helmet": "^4.2.0",
    "jotform": "^0.1.5",
    "nestjs-admin": "^0.4.1",
    "nestjs-typeorm-paginate": "^2.3.2",
    "node-mailjet": "^3.3.1",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/node-mailjet": "^3.3.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.13.x",
    "yarn": "1.x"
  },
  "keywords": [
    "heroku",
    "KANMU",
    "ESPORT",
    "API"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
